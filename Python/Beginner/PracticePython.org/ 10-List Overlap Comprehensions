#task: https://www.practicepython.org/exercise/2014/04/10/10-list-overlap-comprehensions.html

import random
def main():
 ListOverlapComprehensions()
 
def ListOverlapComprehensions():
 a=random.sample(range(1,100),random.randint(1,20))
 b=random.sample(range(1,100),random.randint(1,20))
 print("First list: "+str(a))
 print("Second list: "+str(b))
 print(set(x for x in a if x in b))
main()
